#include <iostream>
using namespace std;

int main(){
     int i;
     float f;
     char s[20];
     
     cout << "bit tam sayi bir reel sayi ve bir kaar girin:" ;
     cin >> i >> f >> s;
     cout << "\n";
     cout << "girdiginiz degerler:"  << i << ' ' << f <<' ' << s << "\n" ;
     return 0;
}